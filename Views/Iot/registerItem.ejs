<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <%- include('../head') %>
  <meta charset="utf-8">
  <title>Product Registration</title>
  <link rel="stylesheet" href="/CSS/iot.css"/>
</head>

<body>
  <div id="content">
    <%- include('nav') %>
    <div class="container">
      <div class="container mb-3">
        <h2 class="mb-3">Register Product</h2>
      </div>
      <form action="registerItem" method="POST">
        <div class="table-responsive">
          <table class="board-table table">
            <thead class="thead-dark">
            <tr>
              <th>Info List</th>
              <th>Product Info</th>
            </tr>
            </thead>
            <tbody class="table-bg">
            <tr>
              <th>RFID</th>
              <td><input type="text" name="rfid" class="box"></td>
            </tr>
            <tr>
              <th>Product Name</th>
              <td><input type="text" name="name" class="box"></td>
            </tr>
            <tr>
              <th>Product Quantity</th>
              <td><input type="number" name="num" class="box"></td>
            </tr>
            </tbody>
          </table>
        </div>
        <button class="btn btn-primary two_btn">Save</button>
      </form>
      <button class="btn btn-primary two_btn" onclick="location.href='warehousing'">Back</button>
    </div>
  </div>
</body>
</html>
