<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <%- include('../../head') %>
</head>

<body>
  <div class="wrap">
    <header>
      <%- include('../../nav_Bar') %>
      <div class="page-header page-header-small">
        <div class="page-header-image" data-parallax="true" style="background-image: url('/Image/about_WH.jpg');"></div>
      </div>
    </header>
    <section>
      <div class="container">
        <h2 class="mt-5 mb-3">IoT Service Request List</h2>
        <table class="board-table table ">
          <thead class="thead-dark">
            <tr class="d-flex">
              <th class="col-2">Warehouse ID</th>
              <th class="col-2">State</th>
              <th class="col-2">User ID</th>
              <th class="col-2">Warehouse Name</th>
              <th class="col-3">Warehouse Address</th>
              <th class="col-1">Nation</th>
              <th class="col-1"></th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i < Object.keys(items).length; i++) { %>
            <tr class="table-warning d-flex">
              <td class="col-2"><%= items['item' + i].warehouseID %></td>
              <td class="col-2">Waiting for Approval</td>
              <td class="col-2"><%= items['item' + i].memberID %></td>
              <td class="col-2"><%= items['item' + i].name %></td>
              <td class="col-3"><%= items['item' + i].address %></td>
              <td class="col-1"><%= items['item' + i].national %></td>
              <td id="td-btn" class="col-1">
                <form action="IoTTest" method="POST">
                  <input type="hidden" name="wid" value="<%= items['item' + i].warehouseID %>">
                  <button class="btn btn-td btn-main">View</button>
                </form>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </section>
    <footer>
      <%- include('../../footer') %>
    </footer>
  </div>
</body>

</html>
